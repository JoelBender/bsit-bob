<http://data.ashrae.org/standard223/1.0/data/test_connection-008>
  a owl:Ontology ;
  rdfs:isDefinedBy <http://data.ashrae.org/standard223/1.0/sample/test_connection-008> ;
  rdfs:label "test_connection-008" ;
  owl:imports <http://data.ashrae.org/standard223/1.0/model/all> .

ex42:00001 a s223:Connection ;
    rdfs:label "Sun" ;
    s223:connectsAt ex42:00003 ;
    s223:connectsFrom ex42:00002 ;
    s223:hasMedium s223:Light-Visible .

ex42:00002 a p223:Light,
        s223:Connectable,
        s223:Equipment ;
    rdfs:label "WL" ;
    s223:connectedThrough ex42:00001 ;
    s223:hasConnectionPoint ex42:00003,
        ex42:00004 ;
    ex8:electricalInlet ex42:00004 ;
    ex8:lightOutlet ex42:00003 .

ex42:00003 a s223:ConnectionPoint,
        s223:OutletConnectionPoint,
        bob:LightVisibleConnectionPoint ;
    rdfs:label "WL.lightOutlet" ;
    s223:connectsThrough ex42:00001 ;
    s223:hasMedium s223:Light-Visible ;
    s223:isConnectionPointOf ex42:00002 .

ex42:00004 a s223:ConnectionPoint,
        s223:InletConnectionPoint,
        bob:ElectricalConnectionPoint ;
    rdfs:label "WL.electricalInlet" ;
    s223:hasMedium s223:Constituent-Electricity ;
    s223:isConnectionPointOf ex42:00002 .

