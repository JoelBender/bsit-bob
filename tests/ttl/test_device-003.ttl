# baseURI: http://data.ashrae.org/standard223/1.0/sample/test_device-003
# imports: http://data.ashrae.org/standard223/1.0/model/all

@prefix bob: <http://data.ashrae.org/standard223/si-builder#> .
@prefix ex18: <urn:ex/test_device-003/> .
@prefix ex42: <urn:ex/test_system-007/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix p223: <http://data.ashrae.org/proposal-to-standard223#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix qudtqk: <http://qudt.org/vocab/quantitykind/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix scratch: <http://data.ashrae.org/standard223/si-builder/prototype#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .




<http://data.ashrae.org/standard223/1.0/data/test_device-003>
  a owl:Ontology ;
  rdfs:isDefinedBy <http://data.ashrae.org/standard223/1.0/sample/test_device-003> ;
  rdfs:label "test_device-003" ;
  owl:imports <http://data.ashrae.org/standard223/1.0/model/all> .

ex42:00001 a s223:Connectable,
        s223:Equipment,
        scratch:ParticleCounter ;
    rdfs:label "PM-1" ;
    s223:contains ex42:00005,
        ex42:00007,
        ex42:00009 ;
    s223:hasConnectionPoint ex42:00002,
        ex42:00003 ;
    rdfs:comment "Particulate Measurement Station AKA particle counter" ;
    ex18:airInlet ex42:00002 ;
    ex18:airOutlet ex42:00003 .

ex42:00002 a s223:ConnectionPoint,
        s223:InletConnectionPoint,
        bob:AirConnectionPoint ;
    rdfs:label "PM-1.airInlet" ;
    s223:hasMedium s223:Fluid-Air ;
    s223:isConnectionPointOf ex42:00001 .

ex42:00003 a s223:ConnectionPoint,
        s223:OutletConnectionPoint,
        bob:AirConnectionPoint ;
    rdfs:label "PM-1.airOutlet" ;
    s223:hasMedium s223:Fluid-Air ;
    s223:isConnectionPointOf ex42:00001 .

ex42:00004 a p223:ParticulateCount,
        s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty ;
    rdfs:label "observed_property" ;
    s223:ofMedium s223:Fluid-Air ;
    s223:ofSubstance s223:Particulate-PM10.0 ;
    qudt:hasQuantityKind qudtqk:NumberDensity ;
    qudt:hasUnit unit:NUM-PER-M3 .

ex42:00005 a s223:Connectable,
        s223:Equipment,
        s223:ParticulateSensor,
        s223:Sensor ;
    rdfs:label "coarse_sensor" ;
    s223:hasProperty ex42:00004 ;
    s223:observes ex42:00004 ;
    rdfs:comment "Coarse Particulate Sensor" .

ex42:00006 a p223:ParticulateCount,
        s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty ;
    rdfs:label "observed_property" ;
    s223:ofMedium s223:Fluid-Air ;
    s223:ofSubstance s223:Particulate-PM2.5 ;
    qudt:hasQuantityKind qudtqk:NumberDensity ;
    qudt:hasUnit unit:NUM-PER-M3 .

ex42:00007 a s223:Connectable,
        s223:Equipment,
        s223:ParticulateSensor,
        s223:Sensor ;
    rdfs:label "fine_sensor" ;
    s223:hasProperty ex42:00006 ;
    s223:observes ex42:00006 ;
    rdfs:comment "Fine Particulate Sensor" .

ex42:00008 a p223:ParticulateCount,
        s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty ;
    rdfs:label "observed_property" ;
    s223:ofMedium s223:Fluid-Air ;
    s223:ofSubstance s223:Particulate-PM1.0 ;
    qudt:hasQuantityKind qudtqk:NumberDensity ;
    qudt:hasUnit unit:NUM-PER-M3 .

ex42:00009 a s223:Connectable,
        s223:Equipment,
        s223:ParticulateSensor,
        s223:Sensor ;
    rdfs:label "ultrafine_sensor" ;
    s223:hasProperty ex42:00008 ;
    s223:observes ex42:00008 ;
    rdfs:comment "Ultra Fine Particulate Sensor" .

