# baseURI: http://data.ashrae.org/standard223/1.0/sample/test_sensor-001
# imports: http://data.ashrae.org/standard223/1.0/model/all

@prefix bob: <https://github.com/JoelBender/bsit-bob/> .
@prefix ex42: <urn:ex/test_system-007/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix qudtqk: <http://qudt.org/vocab/quantitykind/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .




<http://data.ashrae.org/standard223/1.0/data/test_sensor-001>
  a owl:Ontology ;
  rdfs:isDefinedBy <http://data.ashrae.org/standard223/1.0/sample/test_sensor-001> ;
  rdfs:label "test_sensor-001" ;
  owl:imports <http://data.ashrae.org/standard223/1.0/model/all> .

ex42:00001 a s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty,
        bob:Temperature ;
    rdfs:label "observed_property" ;
    s223:ofMedium s223:Fluid-Air ;
    qudt:hasQuantityKind qudtqk:Temperature ;
    qudt:hasUnit unit:DEG_C .

ex42:00002 a s223:Connectable,
        s223:Equipment,
        s223:Sensor,
        s223:TemperatureSensor ;
    rdfs:label "ats1" ;
    s223:hasProperty ex42:00001 ;
    s223:observes ex42:00001 ;
    rdfs:comment "Supply Air Temperature Sensor" .

ex42:00003 a s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty,
        bob:Temperature ;
    rdfs:label "observed_property" ;
    s223:ofMedium s223:Fluid-Air ;
    qudt:hasQuantityKind qudtqk:Temperature ;
    qudt:hasUnit unit:DEG_C .

ex42:00004 a s223:Connectable,
        s223:Equipment,
        s223:Sensor,
        s223:TemperatureSensor,
        bob:AirTemperatureSensor ;
    rdfs:label "ats2" ;
    s223:hasProperty ex42:00003 ;
    s223:observes ex42:00003 ;
    rdfs:comment "Supply Air Temperature Sensor" .

ex42:00005 a s223:ObservableProperty,
        s223:Property,
        s223:QuantifiableObservableProperty,
        s223:QuantifiableProperty,
        bob:RelativeHumidity ;
    rdfs:label "observed_property" ;
    s223:hasValue 20.0 ;
    s223:ofMedium s223:Fluid-Air ;
    qudt:hasQuantityKind qudtqk:RelativeHumidity ;
    qudt:hasUnit unit:PERCENT_RH .

ex42:00006 a s223:Connectable,
        s223:Equipment,
        s223:HumiditySensor,
        s223:Sensor ;
    rdfs:label "ahs1" ;
    s223:hasProperty ex42:00005 ;
    s223:observes ex42:00005 ;
    rdfs:comment "Zone Humidity Sensor with a value of 20" .

